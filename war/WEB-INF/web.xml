<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>WeatherServlet</servlet-name>
		<servlet-class>com.kevinachoolhun.suggestws.controller.WeatherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WeatherServlet</servlet-name>
		<url-pattern>/weather</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LocationServlet</servlet-name>
		<servlet-class>com.kevinachoolhun.suggestws.controller.LocationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LocationServlet</servlet-name>
		<url-pattern>/location</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
